{
    "name": "drmonkeyninja/cakephp-inline-css",
    "description": "CakePHP inline CSS plugin",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "css", "helper"],
    "homepage": "https://github.com/drmonkeyninja/cakephp-inline-css",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Carter",
            "role": "Author"
        }
    ],
    "require": {
        "cakephp/cakephp": "~3.0",
        "tijsverkoyen/css-to-inline-styles": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "InlineCss\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "InlineCss\\Test\\": "tests"
        }
    }
}
